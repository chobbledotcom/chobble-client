{
  "$schema": "https://biomejs.dev/schemas/2.3.11/schema.json",
  "extends": ["./node_modules/@chobble/js-toolkit/configs/biome.base.json"],
  "files": {
    "includes": ["scripts/**/*.js"],
    "ignoreUnknown": true
  },
  "overrides": [
    {
      "includes": ["scripts/**/*.js"],
      "linter": {
        "rules": {
          "suspicious": {
            "noConsole": "off"
          }
        }
      }
    }
  ]
}
